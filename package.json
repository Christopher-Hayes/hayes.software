{
  "name": "hayes-software",
  "description": "Personal site for Chris Hayes running 11ty",
  "version": "1.9.8",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:stefanfrede/11st-starter-kit.git"
  },
  "keywords": [
    "alpinejs",
    "eleventy",
    "jamstack",
    "liquidjs",
    "netlify",
    "pwa",
    "tailwindcss",
    "vite"
  ],
  "author": {
    "name": "Chris Hayes",
    "email": "chris@hayes.software"
  },
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "*.html": "prettier --write",
    "*.css": "stylelint --cache --fix",
    "*.js": "eslint --cache --fix"
  },
  "scripts": {
    "clean": "rimraf _site",
    "prepare": "npx husky install",
    "preview": "npx vite preview",
    "predev": "run-s clean",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:eleventy": "npx @11ty/eleventy --incremental --watch",
    "dev:vite": "npx vite --port 3000",
    "prebuild": "run-s clean",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:eleventy": "npx @11ty/eleventy",
    "build:vite": "npx vite build",
    "lint": "run-s lint:*",
    "lint:html": "npx prettier --check \"src/**/*.html\"",
    "lint:stylelint": "npx stylelint \"src/**/*.css\"",
    "lint:eslint": "npx eslint --ext .js src/",
    "format": "run-s format:*",
    "format:html": "npx prettier --write \"src/**/*.html\"",
    "format:stylelint": "npx stylelint \"src/**/*.css\" --fix",
    "format:eslint": "npx eslint --ext .js src/ --fix",
    "validate": "run-s build lint",
    "setup": "npm install && npm run validate",
    "unsplash": "node scripts/getUnsplashPhoto.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@tailwindcss/typography": "^0.5.8",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.0",
    "browserslist": "^4.21.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.14",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-no-unsupported-browser-features": "^6.0.1",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.13.3"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.10.5",
    "@alpinejs/focus": "^3.10.5",
    "alpinejs": "^3.10.5",
    "core-js": "^3.26.1",
    "dotenv": "^16.0.3"
  }
}
